    & .card-question {
        @apply relative items-center overflow-hidden justify-center w-full px-2 py-3 block shrink-0 h-1/3;

        & .bg-card{
            @apply w-full h-full relative;
        }
        & .card-back-1{
            @apply absolute inset-0 z-10 bg-gray-100 shadow rounded-xl rotate-2 dark:bg-palette-dark-tint/70 dark:shadow-black/50;
            content:'';
        }
        & .card-back-2{
            @apply absolute inset-y-0 z-20 bg-gray-100 shadow inset-x-3 rounded-xl -rotate-1 dark:bg-palette-dark-active dark:shadow-black/50;
            content:'';
        }
        & .card-content{
            @apply shadow-md rounded-xl relative z-30 w-full h-full xs:text-sm xs:leading-4 text-xl text-center xs:px-5 px-4 py-4 overflow-hidden bg-white dark:bg-palette-dark-tint dark:shadow-black/50 dark:text-gray-300 tracking-tight leading-tight;
        }

        & .related-content {
            @apply rounded-sm text-left text-xxs font-serif flex items-start w-full justify-center relative leading-tight;

           & img {
               @apply w-auto mx-auto rounded-lg shadow border-2 border-white h-full;
                max-height: 6rem;
            }



        }
        div.pregunta {
            @apply w-4/5 m-auto text-center overflow-hidden
        }

       & .statsRespuestas {
           @apply absolute w-20 h-20 z-50 rounded-full inset-y-2/4 m-auto flex items-center;

            div {
                @apply flex items-center text-center justify-end absolute h-full top-0 left-0 w-6 pt-2 text-2xl;

                & .icon {
                    @apply absolute w-auto top-3 right-0 text-xxs text-center;
                }
            }


            &.correctas {
                @apply bg-lime-200 text-lime-700 left-0;
                transform: translateX(-3.05rem);

                & div {
                    @apply left-auto right-0 justify-start;

                    .icon {
                        right: auto;
                        left: -.3rem;
                    }
                }

            }

            &.incorrectas {
                @apply bg-red-200 text-red-600 right-0;
                transform: translateX(3.05rem);
            }
        }

        .defiantAvatar{
          position: absolute;
          left: 0px;
          top: 0px;

          a{
            padding: 0px;
            background: white;
          }
        }

        .opponentAvatar{
          position: absolute;
          right: 0px;
          top: 0px;

          a{
            padding: 0px;
            background: white;
          }
        }
    }

    & .trivia-answers {
        @apply grid content-center w-full h-full grid-cols-1 grid-rows-4 gap-2 py-2 md:grid-cols-2;

        & .answer-btn{
            @apply shadow relative flex items-center justify-start gap-3 px-4 py-2 overflow-hidden rounded-lg cursor-pointer w-full h-full text-gray-800 shadow-gray-300/40 bg-white dark:bg-palette-dark-tint dark:shadow-black/50 dark:text-gray-100;

            &:active {
                @apply transform scale-95 opacity-100;
              }

            &.active {
                @apply bg-orange-200 border-orange-300 text-orange-700;

                & .number-option {
                    @apply border-orange-700 from-orange-500 to-orange-600 text-white;
                }

                &:before {
                    @apply transition-all font-display absolute top-1 right-1 opacity-50 text-xxs text-orange-500 dark:text-orange-600 text-right uppercase ;
                    content: 'Seleccionada';
                }
            }

            &.answer-success {
                @apply bg-lime-200 border-lime-300 dark:border-lime-300 text-lime-700;

                &:before {
                    @apply font-display absolute top-1 right-1 opacity-40 text-xxs text-lime-800 dark:text-lime-800 text-opacity-40 text-right uppercase;
                    content: 'Correcta';
                }

                & .number-option {
                    @apply border-lime-700 from-lime-500 to-lime-600 text-white;
                }
            }

            &.answer-fail {
                @apply bg-red-200 dark:bg-red-200 border-red-300 dark:border-red-300 text-red-600;

                &:before {
                    @apply font-display absolute top-1 right-1 opacity-40 text-xxs text-red-800 text-opacity-40 text-right uppercase;
                    content: 'Incorrecta';
                }

                & .number-option {
                    @apply border-red-700 from-red-500 to-red-600 text-white;
                }
            }
        }

            @keyframes correcta {
                0% {
                    background-color: #BFF199;
                }

                50%{
                    background-color: #B8E986;
                }

                100% {
                    background-color: #BADC58;
                }
            }

            .flashing {
                animation: correcta 2s linear infinite;
            }
    }

    & .trivia-footer {
        @apply w-full lg:shadow-2xl items-center flex-none relative flex flex-col justify-between h-20 text-white bg-indigo-800 dark:bg-midnight shrink-0 ;

        & .content{
            @apply flex-1 w-full flex justify-between h-full px-3;
        }

        &.respuesta-success {
            @apply bg-quiz-success;
        }

        &.respuesta-fail {
            @apply bg-quiz-fail;
        }

        & .btnJokers{
            @apply flex items-center justify-center relative h-auto w-24 text-xxs uppercase font-display rounded focus:bg-gray-300;

         & .notification-jokers{
             @apply absolute gap-2 left-1 top-0 flex flex-grow-0 items-center justify-center p-1 text-white bg-red-600 rounded-full w-4 h-4 font-display;
         }
        }

        .wrapperTimerTrivia {
            @apply relative flex w-full flex-grow justify-center items-center;

           & .timerTrivia {
                @apply bg-transparent h-16 w-16 flex justify-center items-center transition-all rounded-full border-4 border-gray-200;

               & h3 {
                    @apply text-lg m-0;
                }

                &.border-success {
                    border-color: rgba(#fff, .8) !important;
                    background: rgba(#fff,.5);
                    font-size: 3rem;

                    & .success {
                        color: #A5DC86;
                    }
                }

                &.border-fail {
                    border-color: rgba(#C80000, .2);
                    background: rgba(#fff,.5);
                    font-size: 3rem;

                    & .fail {
                        color: #F27474;
                    }
                }

            }
        }

        .wrapperPreguntasRespondidas {
            @apply relative w-full flex items-center flex-1;

            .preguntasRespondidas {
                @apply mx-2 w-full rounded text-xs text-indigo-500 dark:text-indigo-400 tracking-tight text-center uppercase p-2 flex items-center justify-around font-display;
            }
        }

        .naranjoGradient {
            color: linear-gradient(180deg, #FAD961 0%, #F76B1C 100%);
        }

        &.correcta {
            background: #BFF199;
        }

        &.errada {
            background: #FFD3D1;
        }

        div.checkCorrecta {
            position: absolute;
            top: 50%;
            left: 10%;
            height: 45px;
            width: 45px;
            border-radius: 50%;
            transform: translateY(-50%);
            background: rgba(255, 255, 255, 0.50);
            border: 5px solid rgba(255, 255, 255, 0.80);

            i {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                color: #A5DC86;
                font-size: 30px;
                line-height: 50px;
                opacity: .6;
            }

            svg {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                color: #A5DC86;
                font-size: 30px;
                line-height: 50px;
                opacity: .6;
            }
        }

        div.checkIncorrecta {
            position: absolute;
            top: 50%;
            left: 10%;
            height: 45px;
            width: 45px;
            border-radius: 50%;
            transform: translateY(-50%);
            background: rgba(255, 255, 255, 0.50);
            border: 5px solid rgba(200, 0, 0, 0.20);

            i {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                color: #F27474;
                font-size: 30px;
                line-height: 50px;
                opacity: .6;
            }

            svg {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                color: #F27474;
                font-size: 30px;
                line-height: 50px;
                opacity: .6;
            }
        }

        & .showBotonEnviar {
            @apply z-50 relative flex justify-center items-center w-full mx-5;

            .element {
                animation: shake;
                animation-iteration-count: infinite;
                animation-duration: 4s;
                animation-timing-function: cubic-bezier(0.36, 0.07, 0.19, 0.97);
                animation-delay: 4s;
            }

            @keyframes shake {
              2%, 18% {
                -webkit-transform: translate3d(-1px, 0, 0);
                        transform: translate3d(-1px, 0, 0);
              }
              4%, 16% {
                -webkit-transform: translate3d(2px, 0, 0);
                        transform: translate3d(2px, 0, 0);
              }
              6%, 10%, 14% {
                -webkit-transform: translate3d(-4px, 0, 0);
                        transform: translate3d(-4px, 0, 0);
              }
              8%, 12% {
                -webkit-transform: translate3d(4px, 0, 0);
                        transform: translate3d(4px, 0, 0);
              }
              20%, 100% {
                -webkit-transform: translate3d(0, 0, 0);
                        transform: translate3d(0, 0, 0);
              }
            }
        }


      .element {
                animation: shake;
                animation-iteration-count: infinite;
                animation-duration: 4s;
                animation-timing-function: cubic-bezier(0.36, 0.07, 0.19, 0.97);
                animation-delay: 4s;
      }


      @keyframes shake {
        2%, 18% {
          -webkit-transform: translate3d(-1px, 0, 0);
                  transform: translate3d(-1px, 0, 0);
        }
        4%, 16% {
          -webkit-transform: translate3d(2px, 0, 0);
                  transform: translate3d(2px, 0, 0);
        }
        6%, 10%, 14% {
          -webkit-transform: translate3d(-4px, 0, 0);
                  transform: translate3d(-4px, 0, 0);
        }
        8%, 12% {
          -webkit-transform: translate3d(4px, 0, 0);
                  transform: translate3d(4px, 0, 0);
        }
        20%, 100% {
          -webkit-transform: translate3d(0, 0, 0);
                  transform: translate3d(0, 0, 0);
        }
      }
    }

    &>div.cd-panel__container {
        width: 100% !important;
        border-top-right-radius: 15px;
        border-top-left-radius: 15px;
        margin-top: 60px;

        .cd-panel__content {
            border-top-right-radius: 15px;
            border-top-left-radius: 15px;
        }

        div.contenedorLateral {
            height: 100vh;
            background: #32323F;
            box-shadow: 3px 0 12px 0 rgba(0, 0, 0, 0.40);
            width: 250px;

            div.cabeceraTrivia {
                position: relative;
                height: 35vh;
                background-position: center;
                background-size: cover;
                background-repeat: no-repeat;
                z-index: 2;

                div.logo {
                    position: absolute;
                    top: 5%;
                    left: 5%;

                    img {
                        width: 100px;
                    }
                }

                div.overlay {
                    position: absolute;
                    bottom: -1px;
                    left: 0;
                    width: 100%;
                    height: 50%;
                    background-image: linear-gradient(180deg, rgba(50, 50, 63, 0.00) 0%, #32323F 99%);
                }

                div.titulo {
                    position: absolute;
                    bottom: 8%;
                    left: 5%;
                    width: 60%;
                    z-index: 2;

                    h3 {

                        font-size: 26px;
                        line-height: 26px;
                        color: #FFFFFF;
                        letter-spacing: 0;
                        text-align: left;
                        line-height: 30px;
                        text-shadow: 0 2px 2px rgba(0, 0, 0, 0.20);
                    }

                    span {
                        display: inline-block;
                        margin-bottom: 10px;
                        background-image: linear-gradient(134deg, #FF0467 0%, #FC7437 100%);
                        border-radius: 12.5px;
                        padding: 5px 10px;

                        font-size: 9px;
                        line-height: 9px;
                        color: #FFFFFF;
                        letter-spacing: 0;
                        text-align: center;
                        text-shadow: 0 1px 0 rgba(0, 0, 0, 0.20);
                    }
                }
            }

            div.indiceTrivia {
                height: 65vh;

                h3 {

                    font-size: 18px;
                    color: #DFE6E9;
                    letter-spacing: 0;
                    text-align: left;
                    margin-bottom: 10px;
                }

                ul {
                    padding: unset;
                    width: 70%;
                    margin: auto;

                    li {
                        position: relative;
                        list-style: none;
                        border-radius: 30px;
                        margin-bottom: 7px;
                        padding: 7px 10px 6px;

                        font-size: 13px;
                        line-height: 13px;
                        letter-spacing: 0;

                        &.siguiente {
                            opacity: 0.4;
                            color: #C3CDD1;
                            background: rgba(0, 0, 0, 0.20);

                            span:before {
                                content: '\f075';
                                font-weight: 400;
                            }
                        }

                        &.correcta {
                            color: #6AB04C;
                            background: rgba(0, 0, 0, 0.20);
                            opacity: 1;

                            span:before {
                                content: '\f4ac';
                                font-weight: 400;
                            }
                        }

                        &.errada {
                            color: #EB4D4B;
                            background: rgba(0, 0, 0, 0.20);

                            span:before {
                                content: '\f4b1';
                                font-weight: 400;
                            }
                        }

                        &.actual {
                            color: #4A4A4A;
                            background: #F8E71C;
                            opacity: 1;

                            &:before,
                            &:after {
                                font-family: "Font Awesome 5 Duotone";
                                font-weight: 900;
                                position: absolute;
                                right: 7px;
                                top: 50%;
                                transform: translateY(-50%);
                                width: fit-content;
                                text-align: center;
                                font-size: 15px;
                            }

                            &:before {
                                content: "\f0a4";
                                color: var(--fa-primary-color, inherit);
                                opacity: 1;
                                opacity: var(--fa-primary-opacity, 1.0);
                            }

                            &:after {
                                content: "\10f0a4";
                                color: var(--fa-secondary-color, inherit);
                                opacity: var(--fa-secondary-opacity, 0.4);
                            }

                            span:before {
                                content: '\f4ad';
                                font-weight: 400;
                            }
                        }

                        span {
                            position: relative;
