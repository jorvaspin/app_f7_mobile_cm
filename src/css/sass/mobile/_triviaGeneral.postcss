.page-trivia-mobile {
    & .header-trivia {
        @apply relative flex items-center shrink-0 justify-between text-white px-2 bg-gradient-to-r from-indigo-500 via-purple-500 to-pink-500;

        & .progress-counter{
            @apply absolute bottom-0.5 inset-x-1 w-auto rounded-full h-0.5 bg-white bg-opacity-30 overflow-hidden;
            & .progress-bar{
                @apply  bg-white bg-opacity-60 transition-all ease-out w-full h-full;
            }
        }

        & .correct-answers{
            @apply rounded-xl flex items-center justify-center my-auto w-6 h-6 font-display bg-success-100 p-1.5
        }

        & .wrong-answers{
            @apply rounded-xl flex items-center justify-center w-6 h-6 font-display bg-danger-100 p-1.5
        }

        & .total-questions{
            @apply flex
            justify-center
            items-center
            w-16
            p-1.5
            rounded
            shrink-0
            font-display
            gap-0.5
            shadow
            bg-indigo-600 dark:bg-midnight;
        }

    &.header-nivelation{
        @apply shadow-md bg-gradient-to-r from-nivelation-init to-nivelation-end dark:shadow-gray-900/30;
    }
    &.header-lesson{
        @apply bg-cover bg-bottom bg-no-repeat;

        &:before{
            position: absolute;
            opacity: 0.6;
            content: '';
            top: 0;
            bottom:0;
            left: 0;
            width: 100%;
            background-image: linear-gradient(134deg, #2B0096 0%, #E9649B 100%);
            border-radius: 0 0 10px 10px 10px;
            z-index: 2;
        }
        }

       & .title-tematica{
          margin-top: 2rem;
        }

       & .duelistas{
          display: block;
          text-align: center;
          position: relative;
          height: 60px;
          padding: 27px 4px 5px 4px;

         & .cardTriviaDuelo{
            height: 60px;
            padding: 0px 5px 0px 20px;
            img{
              width: 40px;
              padding: 0px 10px 0px 0px;
              margin-top: 10px;
            }

           & .detalles{
              text-align: left;
             & h4{
                font-size: 12px;
                padding: 0px;
                margin: 0;
              }
             & p{
                padding: 0px;
                margin: 0px;
                font-size: 10px;
              }
             & h5{
                padding: 0px;
                margin: 0px;
                font-size: 10px;
              }
            }
          }

          .alumno {
              position: relative;
              background-image: linear-gradient(135deg, #edf0f6, #c9d6e8);
              box-shadow: 0 3px 0 0 #4b61e6;
              border-radius: 4px;
              padding: 11px 6px 10px 0px;
              transition: all .5s;
              opacity: .7;
              max-width: 200px;
              max-height: 55px;
              margin: auto;

              .avatar {
                  &>img {
                      max-width: 35px;
                      border: 4px solid white;
                      border-radius: 50%;
                      margin: 0px !important;
                      padding: 0px !important;
                  }
              }

              .detalles {
                  padding-top: 3px;

                  small {
                      display: block;

                      font-size: 7px;
                      line-height: 9px;
                      color: #4B61E6;
                      letter-spacing: 0;
                      margin-bottom: 3px;
                  }

                  h4 {

                      font-size: 9px;
                      line-height: 14px;
                      color: #32323F;
                      letter-spacing: 0;
                      text-transform: uppercase;
                      margin-bottom: 4px;
                  }

                  img {
                      width: 12px;
                      height: 12px;
                      margin-right: 5px;

                  }

                  p {

                      font-size: 8px;
                      font-weight: 700;
                      color: #2C2C41;
                      letter-spacing: 0;
                      text-align: left;
                      margin-bottom: 5px;
                      margin: 0;
                  }
              }

              .identificador {
                  position: absolute;
                  top: 0;
                  right: 5px;
                  width: 70px;
                  height: 20px;
                  transform: translateY(-50%);
                  border-radius: 12.5px;

                  font-size: 9px;
                  line-height: 9px;
                  text-transform: uppercase;
                  color: #FFFFFF;
                  letter-spacing: 0;
                  text-align: center;
                  text-shadow: 0 1px 0 rgba(0, 0, 0, 0.20);

                  span {
                      position: absolute;
                      top: 50%;
                      left: 50;
                      transform: translate(-50%, -50%)
                  }

                  &.retador {
                      background-image: linear-gradient(134deg, #4B61E6 0%, #7E41EF 100%);
                  }

                  &.contrincante {
                      background-image: linear-gradient(225deg, #FFD97C 0%, #F6A742 100%);
                  }
              }

              &.active {
                  transform: scale(1.2);
                  opacity: 1;
                  box-shadow: 0 3px 0 0 #B8E986;
              }
          }
        }


    }
    & .header-duels {
        @apply relative flex items-center shrink-0 justify-between text-white px-2 bg-gradient-to-r from-indigo-500 to-cyan-600 to-slate-700;

        & .progress-counter{
            @apply absolute bottom-0.5 inset-x-1 w-auto rounded-full h-0.5 bg-white bg-opacity-30 overflow-hidden;
            & .progress-bar{
                @apply  bg-white bg-opacity-60 transition-all ease-out w-full h-full;
            }
        }

        & .correct-answers{
            @apply rounded-xl flex items-center justify-center my-auto w-6 h-6 font-display bg-success-100 p-1.5
        }

        & .wrong-answers{
            @apply rounded-xl flex items-center justify-center w-6 h-6 font-display bg-danger-100 p-1.5
        }

        & .total-questions{
